pkg_arch="all"
pkg_dirs=`cat <<_EOF_
$vnet_path/vnet/Gemfile
$vnet_path/vnet/Gemfile.lock
$vnet_path/vnet/LICENSE
$vnet_path/vnet/README.md
$vnet_path/vnet/lib
$vnet_path/vnet/vendor
$vnet_path/vnet/db
$vnet_path/vnet/Rakefile
$vnet_path/vnet/.bundle
$etc_path/default/wakame-vnet
_EOF_`
